{
    "name": "covid19",
    "description": "3dbionotes COVID-19",
    "version": "3.10.0",
    "homepage": ".",
    "dependencies": {
        "@dhis2/d2-i18n": "1.0.6",
        "@material-ui/core": "4.12.4",
        "@material-ui/data-grid": "^4.0.0-alpha.31",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/styles": "4.11.2",
        "autosuggest-highlight": "^3.2.1",
        "axios": "^0.21.1",
        "classnames": "^2.3.1",
        "file-saver": "^2.0.5",
        "fluture": "^13.0.1",
        "lnf": "^1.3.10",
        "lodash": "4.17.20",
        "minisearch": "^3.2.0",
        "moment": "^2.29.1",
        "papaparse": "^5.3.1",
        "purify-ts": "^1.1.0",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "styled-components": "^5.3.0",
        "styled-jsx": "3.3.2"
    },
    "scripts": {
        "prestart": "yarn localize",
        "start": "react-scripts start",
        "test": "jest --env=jsdom-fourteen --passWithNoTests",
        "lint": "eslint src cypress --ext .js,.jsx,.ts,.tsx",
        "eject": "react-scripts eject",
        "prettify": "prettier \"{src,config,cypress}/**/*.{js,jsx,ts,tsx,json,css}\" --write",
        "extract-pot": "yarn d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn update-po && d2-i18n-generate -n covid19 -p ./i18n/ -o ./src/locales/",
        "update-po": "yarn extract-pot && find i18n -name '*.po' | while read pofile; do msgmerge --backup=off -U $pofile i18n/en.pot; done",
        "cy:e2e:open": "NODE_ENV=test CYPRESS_E2E=true cypress open",
        "cy:e2e:run": "NODE_ENV=test CYPRESS_E2E=true cypress run --headless --browser chrome",
        "build-inline": "react-scripts build",
        "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && yarn create-build-symlinks",
        "run-ts": "ts-node -P src/scripts/tsconfig.json",
        "create-build-symlinks": "yarn run-ts src/scripts/create-build-symlinks.ts",
        "generate-data-from-json": "yarn run-ts src/scripts/generate-data-from-json.ts"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn prettify && yarn lint && yarn update-po && yarn test",
            "post-merge": "./.githooks/dep-check"
        }
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@babel/preset-typescript": "7.12.7",
        "@dhis2/d2-i18n-extract": "1.0.8",
        "@dhis2/d2-i18n-generate": "1.2.0",
        "@testing-library/jest-dom": "5.11.6",
        "@testing-library/react": "11.2.2",
        "@types/argparse": "2.0.4",
        "@types/autosuggest-highlight": "^3.2.0",
        "@types/classnames": "2.2.11",
        "@types/file-saver": "^2.0.2",
        "@types/jest": "26.0.19",
        "@types/lodash": "4.14.166",
        "@types/material-ui": "0.21.8",
        "@types/mime-types": "^2.1.0",
        "@types/papaparse": "^5.2.5",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-router-dom": "5.1.6",
        "@types/styled-components": "^5.1.10",
        "@typescript-eslint/eslint-plugin": "4.11.1",
        "@typescript-eslint/parser": "4.11.1",
        "babel-core": "6.26.3",
        "babel-eslint": "10.1.0",
        "cypress": "6.2.0",
        "cypress-xpath": "1.6.1",
        "eslint": "7.16.0",
        "eslint-config-react-app": "6.0.0",
        "eslint-plugin-cypress": "2.11.2",
        "eslint-plugin-flowtype": "5.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-react": "7.21.5",
        "eslint-plugin-react-hooks": "4.2.0",
        "husky": "4.3.6",
        "jest": "26.6.3",
        "jest-environment-jsdom-fourteen": "^1.0.1",
        "prettier": "2.2.1",
        "react-test-renderer": "17.0.1",
        "ts-jest": "26.4.4",
        "ts-node": "9.1.1",
        "typescript": "4.3.5",
        "wait-on": "5.2.1"
    }
}
