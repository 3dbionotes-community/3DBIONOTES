                  .index2
                    %a{:href=>"#N"} N
              %tr
                %td.w3-tag.w3-round.w3-blue-grey
                  .index2
                    %a{:href=>"#NSP12"} NSP12
                %td.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#NSP13"} NSP13
                %td.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#NSP14"} NSP14
                %td.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#NSP15"} NSP15
                %td.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#NSP16"} NSP16
                %td.w3-tag.w3-tag.w3-round.w3-white
                %td.w3-tag.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#ORF3a"} ORF3a
                %td.w3-tag.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#E"} E
                %td.w3-tag.w3-tag.w3-round.w3-white
                %td.w3-tag.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#ORF6"} ORF6
                %td.w3-tag.w3-tag.w3-round.w3-white
                %td.w3-tag.w3-tag.w3-round.w3-light-grey
                  .index2
                    %a{:href=>"#ORF7b"} ORF7b
                %td.w3-tag.w3-tag.w3-round.w3-white
