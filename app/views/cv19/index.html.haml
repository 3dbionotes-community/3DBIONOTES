        #cv_dataset_content.w3-container

          #cv_notice.w3-panel-pading.w3-center.w3-sand.w3-leftbar.w3-border-orange.w3-border.w3-round-large
            %p Please note this is an on-going effort to collect all possible relevant information from many diverse sources in a semi-automatic way, sometimes manually.
            %p If you spot something missing, misplaced, incorrect or misbehaving, please feel free to contact us ( 3dbionotes @ cnb.csic.es).

          %h3.w3-center.w3-opacity{style:"font-weight:bold;color:#074987;"} Known Proteins
          %hr.dashed

          - uEntry = "P0DTC1"
          #P0DTC1.data_item_content.w3-card.section#R1a
            %header.w3-padding
              .w3-tag.w3-padding-large.w3-round.w3-blue-grey
                = link_to uEntry, @bionotes_qry_pre+uEntry+@bionotes_qry_sub
              - @alt_names[uEntry].each do |a_name|
                .w3-tag.w3-padding-large.w3-round.w3-light-grey= a_name

              - uEntry = "NSP3"
              #PLPro.data_item_content.w3-card.section#NSP3
                %header.w3-padding
                  .w3-tag.w3-padding-large.w3-round.w3-light-grey= uEntry
                  .w3-tag.w3-padding-large.w3-round.w3-light-grey= 'PL-Pro'
                  .w3-tag.w3-padding-large.w3-round.w3-light-grey= 'Papain-like proteinase'
                #tabs_panel.w3-panel.w3-responsive.w3-border
                  .w3-bar.w3-round.w3-light-grey.w3-border
                    %button.w3-bar-item.w3-button.tablink.w3-grey{onclick:"openTab(event,'NSP3_PDB','nsp3_item_tab')"} PDB
                    %button.w3-bar-item.w3-button.tablink.w3-disabled EMDB
                    %button.w3-bar-item.w3-button.tablink{onclick:"openTab(event,'NSP3_CompModels','nsp3_item_tab')"} Computational Models
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Interactions
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Other Related
                  #NSP3_PDB.nsp3_item_tab.w3-panel.w3-mobile
                    - @nsp3_models.each do | modelId |
                      .models.w3-cell.w3-button.w3-round
                        -img_path = @pdb_img_path_pre + modelId + @pdb_img_path_sub
                        -query_path = @bionotes_qry_pre + modelId + @bionotes_qry_sub
                        = link_to image_tag(img_path, alt: modelId, height: '100'), query_path
                        %p.w3-padding-small{style:"font-weight:bold"}= modelId
                        - if @nsp3_pdb_redo.include? modelId
                          = link_to 'PDB-redo',@bionotes_qry_pre + "PDB-REDO-" + modelId + @bionotes_qry_sub, class: "w3-tag w3-round w3-teal"
                  -# #NSP3_EMDB.nsp3_item_tab.w3-panel.w3-mobile{style: "display:none"}
                  -#   .maps.w3-cell
                  -#     %p <i class="fa fa-spinner w3-spin" style="font-size:32px"></i>
                  #NSP3_CompModels.nsp3_item_tab.w3-panel.w3-mobile{style: "display:none"}
                    #c_models.w3-cell.w3-padding
                      .w3-panel.w3-border
                        %p SWISS-MODEL
                        - @nsp3_c_models_sm.each do |cmodel|
                          .c_models.w3-cell.w3-button.w3-round
                            = link_to image_tag(@swissmodel_path_pre+@nsp3_c_modelset+@swissmodel_path_sub1+cmodel+@swissmodel_path_sub2, alt: 'model #'+cmodel, height: '100'), @swissmodel_path_pre+@nsp3_c_modelset+@swissmodel_path_sub1+cmodel, target: "_blank"
                            %p.w3-padding-small{style:"font-weight:bold"}= 'model #'+cmodel
                            
                  -# #NSP3_Interactions.nsp3_item_tab.w3-panel.w3-mobile{style: "display:none"}
                  -#   .interactions.w3-cell
                  -#     %p <i class="fa fa-spinner w3-spin" style="font-size:32px"></i>
                  -# #NSP3_OtherRelated.nsp3_item_tab.w3-panel.w3-mobile{style: "display:none"}
                  -#   .o_related.w3-cell
                  -#     %p <i class="fa fa-spinner w3-spin" style="font-size:32px"></i>

              - uEntry = "NSP5"
              #PLPro.data_item_content.w3-card.section#NSP5
                %header.w3-padding
                  - @nsp5_names.each do | a_name |
                    .w3-tag.w3-padding-large.w3-round.w3-light-grey= a_name
                #tabs_panel.w3-panel.w3-responsive.w3-border
                  .w3-bar.w3-round.w3-light-grey.w3-border
                    %button.w3-bar-item.w3-button.tablink.w3-grey{onclick:"openTab(event,'NSP5_PDB','nsp5_item_tab')"} PDB
                    %button.w3-bar-item.w3-button.tablink.w3-disabled EMDB
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Computational Models
                    %button.w3-bar-item.w3-button.tablink{onclick:"openTab(event,'NSP5_Interactions','nsp5_item_tab')"} Interactions
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Other Related
                  #NSP5_PDB.nsp5_item_tab.w3-panel.w3-mobile
                    - @nsp5_models2.each do | modelId |
                      .models.w3-cell.w3-button.w3-round
                        -img_path = @pdb_img_path_pre + modelId + @pdb_img_path_sub
                        -query_path = @bionotes_qry_pre + modelId + @bionotes_qry_sub
                        = link_to image_tag(img_path, alt: modelId, height: '100'), query_path
                        %p.w3-padding-small{style:"font-weight:bold"}= modelId
                        - if @nsp5_pdb_redo.include? modelId
                          = link_to 'PDB-redo',@bionotes_qry_pre + "PDB-REDO-" + modelId + @bionotes_qry_sub, class: "w3-tag w3-round w3-teal"
                  #NSP5_Interactions.nsp5_item_tab.w3-panel.w3-mobile{style: "display:none"}
                    #interactions.w3-cell.w3-padding
                    .w3-panel
                      %p Ligands
                      - @nsp5_inter_ligands.each do | modelId |
                        .models.w3-cell.w3-button.w3-round
                          -img_path = @pdb_img_path_pre + modelId + @pdb_img_path_sub
                          -query_path = @bionotes_qry_pre + modelId + @bionotes_qry_sub
                          = link_to image_tag(img_path, alt: modelId, height: '100'), query_path
                          %p.w3-padding-small{style:"font-weight:bold"}= modelId

          .w3.panel
            %p
          - uEntry = "P0DTD1"
          #P0DTD1.data_item_content.w3-card.section#R1ab
            %header.w3-padding
              .w3-tag.w3-padding-large.w3-round.w3-blue-grey
                = link_to uEntry, @bionotes_qry_pre+uEntry+@bionotes_qry_sub
              - @alt_names[uEntry].each do |a_name|
                .w3-tag.w3-padding-large.w3-round.w3-light-grey= a_name

              - uEntry = "NSP12"
              #NSP12.data_item_content.w3-card.section#NSP12
                %header.w3-padding
                  - @nsp12_names.each do | a_name |
                    .w3-tag.w3-padding-large.w3-round.w3-light-grey= a_name
                #tabs_panel.w3-panel.w3-responsive.w3-border
                  .w3-bar.w3-round.w3-light-grey.w3-border
                    %button.w3-bar-item.w3-button.tablink.w3-grey{onclick:"openTab(event,'NSP12_PDB','nsp12_item_tab')"} PDB
                    %button.w3-bar-item.w3-button.tablink{onclick:"openTab(event,'NSP12_EMDB','nsp12_item_tab')"} EMDB
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Computational Models
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Interactions
                    %button.w3-bar-item.w3-button.tablink.w3-disabled Other Related
                  #NSP12_PDB.nsp12_item_tab.w3-panel.w3-mobile
                    - @nsp12_models.each do | modelId |
                      .models.w3-cell.w3-button.w3-round
                        -img_path = @pdb_img_path_pre + modelId + @pdb_img_path_sub
                        -query_path = @bionotes_qry_pre + modelId + @bionotes_qry_sub
                        = link_to image_tag(img_path, alt: modelId, height: '100'), query_path
                        %p.w3-padding-small{style:"font-weight:bold"}= modelId
                        - if @nsp12_pdb_redo.include? modelId
                          = link_to 'PDB-redo',@bionotes_qry_pre + "PDB-REDO-" + modelId + @bionotes_qry_sub, class: "w3-tag w3-round w3-teal"
                        - if @nsp12_isolde.include? modelId
                          - @nsp12_isolde.each_with_index do |model, index|
                            - if model == modelId
                              -# .w3-tag.w3-round.w3-cyan= "Isolde - " + @nsp12_isolde_uuid[index]
                              = link_to 'Isolde',@bionotes_qry_pre + "ISOLDE-" + modelId + "&uuid=" + @nsp12_isolde_uuid[index] + @bionotes_qry_sub, class: "w3-tag w3-round w3-cyan"

                  #NSP12_EMDB.nsp12_item_tab.w3-panel.w3-mobile{style: "display:none"}
                    - @nsp12_maps.each do | mapId |
                      .maps.w3-cell.w3-button
                        -img_path = @map_img_path_pre + mapId + @map_img_path_sub1 + mapId[4..8] + @map_img_path_sub2
                        -query_path = @bionotes_qry_pre + mapId + @bionotes_qry_sub
                        = link_to image_tag(img_path, alt: mapId, height: '100'), query_path
                        %p.w3-padding-small{style:"font-weight:bold"}= mapId
                        


          .w3.panel
            %p
          - uEntry = "P0DTC2"
          #P0DTC2.data_item_content.w3-card.section#S
            %header.w3-padding
            .w3-tag.w3-padding-large.w3-round.w3-blue-grey
              = link_to uEntry, @bionotes_qry_pre+uEntry+@bionotes_qry_sub
            - @s_names.each do | a_name |
              .w3-tag.w3-padding-large.w3-round.w3-light-grey= a_name
            #tabs_panel.w3-panel.w3-responsive.w3-border
              .w3-bar.w3-round.w3-light-grey.w3-border
                %button.w3-bar-item.w3-button.tablink.w3-grey{onclick:"openTab(event,'S_PDB','s_item_tab')"} PDB
                %button.w3-bar-item.w3-button.tablink{onclick:"openTab(event,'S_EMDB','s_item_tab')"} EMDB
                %button.w3-bar-item.w3-button.tablink.w3-disabled Computational Models
                %button.w3-bar-item.w3-button.tablink.w3-disabled Interactions
                %button.w3-bar-item.w3-button.tablink.w3-disabled Other Related
              #S_PDB.s_item_tab.w3-panel.w3-mobile
                - @s_pdbs.each do | modelId |
                  .models.w3-cell.w3-button.w3-round
                    -img_path = @pdb_img_path_pre + modelId + @pdb_img_path_sub
                    -query_path = @bionotes_qry_pre + modelId + @bionotes_qry_sub
                    = link_to image_tag(img_path, alt: modelId, height: '100'), query_path
                    %p.w3-padding-small{style:"font-weight:bold"}= modelId
                    - if @s_pdb_redo.include? modelId
                      = link_to 'PDB-redo',@bionotes_qry_pre + "PDB-REDO-" + modelId + @bionotes_qry_sub, class: "w3-tag w3-round w3-teal"
                    - if @s_isolde.include? modelId
                      - @s_isolde.each_with_index do |model, index|
                        - if model == modelId
                          -# .w3-tag.w3-round.w3-cyan= "Isolde - " + @nsp12_isolde_uuid[index]
                          = link_to 'Isolde',@bionotes_qry_pre + "ISOLDE-" + modelId + "&uuid=" + @s_isolde_uuid[index] + @bionotes_qry_sub, class: "w3-tag w3-round w3-cyan"

